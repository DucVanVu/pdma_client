// UI Sortable

.ui-sortable-helper {
  cursor: move;
}

.list.ui-sortable {
  list-style: none outside none;
  padding-left: 0 !important;

  & > .item {
    width: 100%;
    padding: 0px;
    margin: 5px 0;
    border: 1px solid #dddddd;
    background-color: white;

    text-align: left;
    cursor: pointer;
    display: table;

    & > span {
      display: table-cell;
      padding: 5px;
      font-weight: bold;

      &:first-child {
        width: 20px;
        background-color: #eee;
        letter-spacing: 1px;
        color: #999;
      }
      &:nth-child(2) {
        width: calc(100% - 120px);
      }
      &:last-child {
        width: 100px;
        & > i {
          color: #aaa;
        }
      }
    }
  }
}

