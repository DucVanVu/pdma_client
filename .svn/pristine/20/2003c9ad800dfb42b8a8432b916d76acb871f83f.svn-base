/* Login page */
.login {
  background-color: #3d3d3d !important;
  background-image: url("../../assets/images/login-bg.jpg?v=2");
  background-size: cover;
  background-position: 0 0;
  background-repeat: no-repeat;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;

  .logo {
    margin-top: 0 !important;
    & img {
      -webkit-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
      -moz-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
      -khtml-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
      box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
    }
  }

  .content {

    & > .pannel-left {
      width: 45%;
      background: url("../../assets/images/crowd.jpg") no-repeat 50% 50%;
      background-size: auto;
      @include transitioning(0.5s)
    }

    & > .pannel-right {
      padding: 0;
      width: 55%;
      @include transitioning(0.5s)
    }

    @media (max-width: 768px) {
      & > .pannel-left {
        width: 0%;
      }

      & > .pannel-right {
        width: 100%;
      }

      width: 400px;
    }

    .form-title {
      color: #363636 !important;
    }

    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content: space-between;
    align-items: stretch;
    margin-top: 0 !important;
    width: 700px;

    @include rounded(7px);
    margin: 0 auto 10px !important;
    padding: 0;

    -webkit-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
    -moz-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
    -khtml-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
    box-shadow: 0 22px 43px rgba(0, 0, 0, 0.5) !important;
    -webkit-transform: translateY(-6px) !important;
    -moz-transform: translateY(-6px) !important;
    -o-transform: translateY(-6px) !important;
    -ms-transform: translateY(-6px) !important;
    transform: translateY(-6px) !important;

    .create-account {
      margin: 0;
      background-color: #4d4d4d;
      @include rounded-bottom-left(0);
      @include rounded-bottom-right(7px);
    }

    .login-options {
      margin-bottom: 10px !important;
      padding-left: 30px;
      padding-right: 30px;
    }

    .form-group {
      padding-left: 30px;
      padding-right: 30px;
    }

    .form-actions {
      text-align: right;
      white-space: nowrap;
      margin: 0;
    }

    .form-control {
      border: 1px solid #dddddd !important;
      background-color: #ececec !important;
    }
  }

  .copyright {
    margin-bottom: 10px !important;
  }
}

