@mixin rounded($radius) {
  border-radius: $radius !important;
  -webkit-border-radius: $radius !important;
  -moz-border-radius: $radius !important;
  -ms-border-radius: $radius !important;
}

@mixin rounded-top($radius) {
  border-top-left-radius: $radius !important;
  border-top-right-radius: $radius !important;
  -webkit-border-top-left-radius: $radius !important;
  -webkit-border-top-right-radius: $radius !important;
  -moz-border-radius-topright: $radius !important;
  -moz-border-radius-topleft: $radius !important;
}

@mixin rounded-top-left($radius) {
  border-top-left-radius: $radius !important;
  -webkit-border-top-left-radius: $radius !important;
  -moz-border-radius-topleft: $radius !important;
}

@mixin rounded-top-right($radius) {
  border-top-right-radius: $radius !important;
  -webkit-border-top-right-radius: $radius !important;
  -moz-border-radius-topright: $radius !important;
}

@mixin rounded-bottom-left($radius) {
  border-bottom-left-radius: $radius !important;
  -webkit-border-bottom-left-radius: $radius !important;
  -moz-border-radius-bottomleft: $radius !important;
}

@mixin rounded-bottom-right($radius) {
  border-bottom-right-radius: $radius !important;
  -webkit-border-bottom-right-radius: $radius !important;
  -moz-border-radius-bottomright: $radius !important;
}

@mixin rounded-bottom($radius) {
  border-bottom-left-radius: $radius !important;
  border-bottom-right-radius: $radius !important;
  -webkit-border-bottom-left-radius: $radius !important;
  -webkit-border-bottom-right-radius: $radius !important;
  -moz-border-radius-bottomright: $radius !important;
  -moz-border-radius-bottomleft: $radius !important;
}

@mixin rounded-bottom($radius) {
  border-bottom-left-radius: $radius !important;
  border-bottom-right-radius: $radius !important;
  -webkit-border-bottom-left-radius: $radius !important;
  -webkit-border-bottom-right-radius: $radius !important;
  -moz-border-radius-bottomright: $radius !important;
  -moz-border-radius-bottomleft: $radius !important;
}

@mixin rounded-left($radius) {
  border-bottom-left-radius: $radius !important;
  border-top-left-radius: $radius !important;
  -webkit-border-bottom-left-radius: $radius !important;
  -webkit-border-top-left-radius: $radius !important;
  -moz-border-radius-topleft: $radius !important;
  -moz-border-radius-bottomleft: $radius !important;
}

@mixin rounded-right($radius) {
  border-bottom-right-radius: $radius !important;
  border-top-right-radius: $radius !important;
  -webkit-border-bottom-right-radius: $radius !important;
  -webkit-border-top-right-radius: $radius !important;
  -moz-border-radius-topright: $radius !important;
  -moz-border-radius-bottomright: $radius !important;
}

@mixin transitioning($seconds) {
  transition: all $seconds !important;
  -webkit-transition: all $seconds !important;
  -moz-transition: all $seconds !important;
}

@mixin translateX($pixel) {
  -webkit-transform: translateX($pixel);
  -moz-transform: translateX($pixel);
  -o-transform: translateX($pixel);
  -ms-transform: translateX($pixel);
  transform: translateX($pixel);
}

@mixin translateY($pixel) {
  -webkit-transform: translateY($pixel);
  -moz-transform: translateY($pixel);
  -o-transform: translateY($pixel);
  -ms-transform: translateY($pixel);
  transform: translateY($pixel);
}

/** Variables **/

$patient-status-normal: #337AB7;
$patient-status-not-synced: #e02222;
$patient-status-ltfu: #e02222;
$patient-status-dead: #888;
$patient-status-trans-out: #9500ff;
$patient-status-trans-in: #2c4762;
$patient-status-record-deleted: #dddddd;

a {
  &.patient-status-normal {
    color: $patient-status-normal;
  }

  &.patient-status-ltfu {
    color: $patient-status-ltfu;
  }

  &.patient-status-dead {
    color: $patient-status-dead;
  }

  &.patient-status-trans-out {
    color: $patient-status-trans-out;
  }

  &.patient-status-enrollment-pending {
    color: $patient-status-trans-in;
  }

  &.no-decoration {
    &:hover, &:visited, &:active {
      text-decoration: none;
    }

    &[disabled="disabled"] {
      border: none;
      background-color: transparent;
      color: #aaaaaa !important;
    }

    &:not([disabled="disabled"]) {
      animation: bouncing 0.5s cubic-bezier(0.1, 0.25, 0.1, 1) 0s 2 alternate both;
      -webkit-animation: bouncing 0.5s cubic-bezier(0.1, 0.25, 0.1, 1) 0s 2 alternate both;

      &:hover {
        -webkit-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;
        -moz-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;
        -khtml-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;
        box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;

        background-color: #36c6d3;
        padding: 5px 10px;
        color: #ffffff;

        @include rounded-bottom-left(10px);
        @include rounded-top-right(10px);
      }
    }
  }
}

