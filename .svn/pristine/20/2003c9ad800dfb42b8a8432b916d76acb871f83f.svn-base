        margin: 5px 0 10px 0 !important;
        position: relative;
        border: none !important;

        /*Font*/
        font-size: 16px;
        line-height: 1.2;
        color: #666;
        text-align: justify;

        &::before {
          content: "\201C"; /*Unicode for Left Double Quote*/

          /*Font*/
          font-family: Georgia, serif;
          font-size: 60px;
          font-weight: bold;
          color: #eee;

          /*Positioning*/
          position: absolute;
          left: 5px;
          top: 5px;
        }

        &::after {
          /*Reset to make sure*/
          content: "";
        }

      }
