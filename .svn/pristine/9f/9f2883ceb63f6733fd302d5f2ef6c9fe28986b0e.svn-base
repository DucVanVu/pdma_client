                                                        <span class="check"></span>
                                                        <span class="box"></span>
                                                    </label>
                                                </div>
                                                <div class="md-radio">
                                                    <input type="radio" id="vm.entry.c7FeMale" ng-change="vm.c7Change()" name="vm.entry.c7FeMale" ng-model="vm.entry.c7" value="FEMALE" class="md-radiobtn">
