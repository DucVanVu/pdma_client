                                                <ui-select-choices repeat="c.val as c in vm.c5Values | propsFilter: {name: $select.search}">
                                                    <div class="bold" ng-bind-html="c.name | highlight: $select.search"></div>
                                                </ui-select-choices>
                                            </ui-select>
                                        </div>
