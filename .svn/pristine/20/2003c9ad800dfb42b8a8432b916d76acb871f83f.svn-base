textarea.form-control,
div.form-control,
span.form-control,
input.form-control {
  @include rounded(0);
  border-color: #dddddd;
  &.gray {
    background-color: #EFEFEF;
    font-weight: 600;
  }

  &.dashed-border {
    border-style: dashed;

    &:focus {
      border-style: solid;
    }
  }

  &.left-border-only {
    border-top: none;
    border-right: none;
    border-bottom: none;
  }
}

.page-header.navbar .menu-toggler > span, .page-header.navbar .menu-toggler > span:after, .page-header.navbar .menu-toggler > span:before {
  height: 2px;
}

.btn-group > .dropdown-menu:before, .dropdown-toggle > .dropdown-menu:before, .dropdown > .dropdown-menu:before {
  position: absolute;
  top: 0px;
  left: 0px;
  right: auto;
  display: inline-block !important;
  border-right: 0px solid transparent !important;
  border-bottom: 0px solid #e0e0e0 !important;
  border-left: 0px solid transparent !important;
  content: '';
}

.btn-group > .dropdown-menu:after, .dropdown-toggle > .dropdown-menu:after, .dropdown > .dropdown-menu:after {
  position: absolute;
  top: 0px;
  left: 0px;
  right: auto;
  display: inline-block !important;
  border-right: 0px solid transparent !important;
  border-bottom: 0px solid #fff !important;
  border-left: 0px solid transparent !important;
  content: '';
}

.org-selection {
  height: 308px;
  overflow-x: hidden;
  overflow-y: scroll;
  border: 1px solid #dddddd;
  @include rounded(4px);
  padding: 15px;
  & > div {
    width: 100%;
    display: table;
    & > div:first-child {
      display: table-cell;
      width: 25px;
    }
    & > div:last-child {
      display: table-cell;
      width: calc(100% - 26px);
    }

    &:not(:last-child), &:not(:first-child) {
      padding-top: 8px;
      padding-bottom: 8px;
    }

    &:not(:last-child) {
      border-bottom: 1px dashed #dddddd;
    }
  }
}

.flatpickr-input.input {
  font-weight: 400;
}

.icon-muted {
  color: #dfdfdf !important;
}

.icon-muted-alt {
  color: #dddddd !important;
}

.calendar {
  &.event {
    position: absolute;
    width: 3px;
    height: 3px;
    border-radius: 150px;
    bottom: 3px;
    left: calc(50% - 1.5px);
    content: " ";
    display: block;
    background: #3d8eb9;

    &.busy {
      background: #f64747;
    }
  }
}

.ta-editor {
