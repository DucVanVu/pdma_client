                                                    <div  ng-repeat="n in [12,13,14,15]" class="md-checkbox margin-top-20">
                                                        <input data-ng-disabled="vm.entry.c7=='MALE' && n==14" type="checkbox" id="c9_{{vm.c9Values[n].val}}_{{n}}" ng-change="vm.c9CheckChange(vm.c9Values[n],n)" ng-model="vm.c9Values[n].isCheck" class="md-check ng-pristine ng-untouched ng-valid ng-empty">
                                                        <label class="font-weight-500" for="c9_{{vm.c9Values[n].val}}_{{n}}">
                                                            <span></span>
                                                            <span class="check"></span>
                                                            <span class="box"></span> {{n+1}}. {{vm.c9Values[n].name}}
