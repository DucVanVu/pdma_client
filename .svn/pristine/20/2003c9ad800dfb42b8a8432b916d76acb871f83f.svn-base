}

.select-box-container {
  display: flex;
  flex-direction: row;
  position: relative;
  align-items: flex-end;

  & > span {
    white-space: nowrap;
  }

  .btn:not(.md-skip):not(.bs-select-all):not(.bs-deselect-all) {
    padding-left: 5px;
  }

  & > .form-control,
  & > .ui-select-container {
    display: inline-block;
  }

  & > .ui-select-container {
    flex-grow: 1;
  }

  & > .form-control,
  & > .ui-select-container .form-control {
    background-color: transparent !important;
    width: 100% !important;
    border: none;
    display: inline-block;
    color: #0D6AAD;
    font-weight: 600;
    cursor: pointer;
    border-bottom: 1px dashed #aaaaaa;
    @include transitioning(0.5s);

    &:hover {
      //text-decoration: underline;
      background-color: #0D6AAD !important;
      color: #ffffff;

      span {
        color: #ffffff;
      }

      + i.ind {
        color: #ffffff;
      }
    }

    &:focus {
      outline: none;
      background-color: #0D6AAD !important;
      color: #ffffff;

      &::placeholder { /* Chrome, Firefox, Opera, Safari 10.1+ */
        color: #ffffff;
        opacity: 1; /* Firefox */
      }
      &::-ms-input-placeholder { /* Microsoft Edge */
        color: #ffffff;
      }
    }
  }
}

.date-picker-container {
  display: inline-block;
  position: relative;
  & > .ui-select-container {
    display: inline-block;
  }

  & > .as-link {
    background-color: transparent !important;
    //width: auto;
    width: 190px;
    border: none;
    display: inline-block;
    color: #0D6AAD;
    font-weight: 600;
    cursor: pointer;
    @include transitioning(0.5s);

    &.width-auto {
      font-size: 12px !important;
      //border: 1px solid #dddddd;
      border-bottom: none !important;
      width: 120px;
    }

    &.with-border-bottom {
      border-bottom: 1px dashed #aaaaaa;
    }

    &:hover {
      //text-decoration: underline;
      background-color: #0D6AAD !important;
      color: #ffffff;
      + i.ind {
        color: #ffffff;
      }
