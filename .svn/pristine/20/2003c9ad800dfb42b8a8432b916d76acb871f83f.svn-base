.summary-panel {
  .flatpickr-input, .flatpickr-input + input {
    background-color: white;
  }

  .opc-dashboard-tasks {
    @-webkit-keyframes color_change {
      0% {
        color: #1E9B21;
      }
      50% {
        color: #CE8C17
      }
      100% {
        color: #C42C2C;
      }
    }
    @-moz-keyframes color_change {
      0% {
        color: #1E9B21;
      }
      50% {
        color: #CE8C17
      }
      100% {
        color: #C42C2C;
      }
    }
    @-ms-keyframes color_change {
      0% {
        color: #1E9B21;
      }
      50% {
        color: #CE8C17
      }
      100% {
        color: #C42C2C;
      }
    }
    @-o-keyframes color_change {
      0% {
        color: #1E9B21;
      }
      50% {
        color: #CE8C17
      }
      100% {
        color: #C42C2C;
      }
    }
    @keyframes color_change {
      0% {
        color: #1E9B21;
      }
      50% {
        color: #CE8C17
      }
      100% {
        color: #C42C2C;
      }
    }

    min-height: 130px;

    & > .heading {
      color: #C42C2C;
      position: absolute;
      left: 0;
      bottom: 50%;
      font-weight: 600;
      font-size: 16px;
      transform: rotate(-90deg);
      -webkit-transform: rotate(-90deg);
      -moz-transform: rotate(-90deg);
      -ms-transform: rotate(-90deg);
      -o-transform: rotate(-90deg);
      filter: progid:DXImageTransform.Microsoft.BasicImage(rotation=3);

      -webkit-animation: color_change 3s infinite alternate;
      -moz-animation: color_change 3s infinite alternate;
      -ms-animation: color_change 3s infinite alternate;
      -o-animation: color_change 3s infinite alternate;
      animation: color_change 3s infinite alternate;
    }

    & > div:last-child {
      margin-left: 35px;
      border-left: 1px solid #dddddd;
    }

    .task-item {
      //margin-left: 50px;
      margin-top: 10px;
      font-weight: 500;
      padding-bottom: 10px;
      border-bottom: 1px dashed #dddddd;
      @include transitioning(0.5s);

      &:hover {
        cursor: pointer;
        color: #002a80;
        -webkit-transform: translateY(-2px) !important;
        -moz-transform: translateY(-2px) !important;
        -o-transform: translateY(-2px) !important;
        -ms-transform: translateY(-2px) !important;
        transform: translateY(-2px) !important;
      }

      &.first-child {
        margin-top: 20px;
        @media (max-width: 991px) {
          &.width-border-top {
            border-top: 1px dashed #dddddd;
            padding-top: 10px;
            margin-top: 10px !important;
          }
        }
      }
      &.last-child {
        margin-bottom: 15px;
        &.mind-margin-bottom {
          @media (max-width: 991px) {
            border-bottom: none;
            padding-bottom: 0;
            margin-bottom: 0 !important;
            &:hover {
              border: none;
              padding-bottom: 0 !important;
              margin-bottom: 0 !important;
            }
          }
        }
      }

      & > i {
        margin-right: 10px;
      }
    }
  }
}

