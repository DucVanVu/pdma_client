                                            <label class="bold">- Ghi rõ: </label>
                                            <ui-select id="vm.entry.c5Note" ng-model="vm.entry.c5Note" sortable="true" close-on-select="true" style="width: 100%!important;">
                                                <ui-select-match allow-clear="false" placeholder="Xin mời chọn...">{{$select.selected.name}}</ui-select-match>
                                                <ui-select-choices repeat="c.val as c in vm.c5NoteValues | propsFilter: {name: $select.search}">
                                                    <div class="bold" ng-bind-html="c.name | highlight: $select.search"></div>
