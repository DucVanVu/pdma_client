.btn-default-focus {
  background-color: transparent;
  outline: none;
  box-shadow: none;
  -moz-box-shadow: none;
  -o-box-shadow: none;
  -webkit-box-shadow: none;
}

.fixed-element {
  width: 0;
  height: 0;
  position: fixed;
  right: 0;
  top: 0;
  background-color: transparent;
  border: none;
}

.keyboard-key {
  display: inline-block;
  padding: 5px 10px;
  border: 1px solid #dddddd;
  font-family: "Courier New", Courier, monospace;
  font-size: 11px;
  color: #5a5a5a;
