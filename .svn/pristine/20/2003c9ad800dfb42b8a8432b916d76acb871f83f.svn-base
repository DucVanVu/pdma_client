  white-space: nowrap;
  display: inline-block;
  margin-top: 5px;
}

body {
  -webkit-font-smoothing: antialiased !important;
}

hr {
  &.small-margin {
    margin: 5px 0 !important;
  }
}

.btn,
input,
a {
  @include transitioning(0.3s);
}

.tabbable-line {
  @keyframes left-bouncing {
    0% {
      right: 15px;
    }
    100% {
      right: 25px;
    }
  }

  @-webkit-keyframes left-bouncing {
    0% {
      bottom: 0;
      -webkit-box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
    }
    100% {
      bottom: 50px;
      -webkit-box-shadow: 0 50px 50px rgba(0, 0, 0, 0.1);
    }
  }

  & > .btn-close {
    //border: 1px solid #dddddd;
    //padding: 5px 10px;
    position: absolute;
    right: 15px;
    top: 12px;
    height: 24px;
    padding: 3px 10px 0 10px;
    font-size: 14px;
    font-weight: 600;
    color: #344559;
    background-color: #eef1f5;
    @include rounded(12px);
    @include transitioning(.5s);

    &:hover {
      //animation: left-bouncing 0.5s ease-in 0s infinite alternate both;
      //-webkit-animation: left-bouncing 0.5s ease-in 0s infinite alternate both;
      color: #ffffff;
      background-color: #344559;
      text-decoration: none;

      -webkit-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;
      -moz-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;
      -khtml-box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;
      box-shadow: 0 22px 43px rgba(0, 0, 0, 0.15) !important;
      -webkit-transform: translateY(-3px) !important;
      -moz-transform: translateY(-3px) !important;
      -o-transform: translateY(-3px) !important;
      -ms-transform: translateY(-3px) !important;
      transform: translateY(-3px) !important;
    }
  }

  & > .tab-content {
    padding-top: 15px !important;

    &.no-border-top {
      border-top: none !important;
    }
  }
}

.opc-dashlet {
  display: flex;
  flex-direction: row;
  flex-wrap: nowrap;
  align-items: flex-end;
  margin-bottom: 30px;
  min-height: 180px;

  & > div:first-child {
    display: flex;
    flex-direction: column;
    align-items: center;

    & > span:first-child {
      font-size: 85px;
      align-self: center;
      position: relative;

      &.smaller {
        font-size: 70px;
      }

      &.green-text {
        color: #1E9B21;
      }

      &.light-blue-text {
        color: #20639B;
      }

      &.red-text {
        color: #ED553B;
      }

      &.yellow-text {
        color: #F6D55C;
      }

      &.dark-blue-text {
        color: #264652;
      }

      & > .stat {
        font-size: 15px;
        position: absolute;
        top: 15px;
        right: -30px;
      }
    }

    & > span:last-child {
      border-top: 2px solid #dddddd;
      background-color: rgba(0, 0, 0, 0.02);
      padding: 10px 15px;
      font-weight: 500;
      white-space: nowrap;
    }
  }

  & > div:last-child {
    display: flex;
    flex-direction: column;
    align-items: stretch;
    flex-wrap: nowrap;
    margin-left: 5px;

    & > div:first-child {
      border-bottom: 1px solid rgba(0, 0, 0, 0.1);
      padding-bottom: 5px;
      display: flex;
      flex-basis: 100%;
      align-self: center;
    }

    & > div:last-child {
      align-self: flex-end;
      display: flex;
      flex-direction: row;
      align-items: flex-end;
      flex-wrap: nowrap;
      margin-top: 15px;

      & > span {
        display: flex;
        flex-direction: column;
        flex-grow: 1;
        margin-left: 2px;

        & > span:first-child {
          font-weight: 500;
          font-size: 40px;
          color: #555555;
          align-self: center;
          position: relative;

          /**
          Temporarily disable this
           */
          & > i {
            display: none;
            font-style: normal;
            font-size: 13px;
            position: absolute;
            white-space: nowrap;
            right: -20px;
            top: 0;

            & > i {
              font-weight: 400;
              font-size: 11px;
              color: #aaaaaa;
            }
          }
        }
        & > span:last-child {
          border-top: 2px solid #dddddd;
          background-color: rgba(0, 0, 0, 0.02);
          padding: 10px 8px;
          font-weight: 500;
          white-space: nowrap;
        }
      }
    }
  }
}

