                                                        <label class="font-weight-500" for="c9_{{vm.c9Values[n].val}}_{{n}}">
                                                            <span></span>
                                                            <span class="check"></span>
                                                            <span class="box"></span> {{n+1}}. {{vm.c9Values[n].name}}
                                                        </label>
                                                    </div>
                                                </div>
